<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @beer_club.name %>
</p>

<p>
  <strong>Founded:</strong>
  <%= @beer_club.founded %>
</p>

<ul>
  <% @beer_club.memeberships.each do |member| %>
      <li>
        <%= link_to member.user.username, user_path(member.user) %>
      </li>
  <% end %>
</ul>


<p>
  <strong>City:</strong>
  <%= @beer_club.city %>
</p>

<%= link_to 'Edit', edit_beer_club_path(@beer_club) %> |
<%= link_to 'Back', beer_clubs_path %>
